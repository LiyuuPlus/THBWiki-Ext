{
  "name": "__MSG_extName__",
  "version": "1.0.8",
  "default_locale": "zh_CN",
  "description": "__MSG_extDescription__",
  "manifest_version": 2,
  "permissions": [
    "contextMenus",
    "tabs",
    "cookies",
    "*://thwiki.cc/*",
    "notifications",
    "alarms",
    "storage"
  ],
  "homepage_url": "https://github.com/whuihuan/THBWiki-Ext",
  "background": {
    "page": "./pages/background.html"
  },
  "browser_action": {
    "default_title": "THBWiki",
    "default_icon": "public/images/logo-32-bw.png",
    "default_popup": "pages/popup.html"
  },
  "icons": {
    "128": "public/images/logo-128.png"
  },
  "omnibox": {
    "keyword": "thb"
  },
  "content_scripts": [
    {
      "matches": [
        "*://thwiki.cc/*"
      ],
      "css": [
        "./public/libs/element-ui/element-ui.css",
        "./public/css/style.css"
      ],
      "js": [
        "./public/libs/jquery-3.3.1.min.js",
        "./public/libs/vue.min.js",
        "./public/libs/element-ui/element-ui.js",
        "./public/js/common.js",
        "./public/js/ext.js"
      ]
    }
  ],
  "web_accessible_resources": [
      "public/images/char/*"
  ],
  "content_security_policy": "style-src 'self' 'unsafe-inline';script-src 'self' 'unsafe-eval'; object-src 'self' ;img-src *;"
}